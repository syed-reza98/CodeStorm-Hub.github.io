<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="cloudGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f093fb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5576c;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="400" height="300" fill="url(#cloudGrad)"/>
  
  <!-- Cloud illustrations -->
  <g fill="rgba(255,255,255,0.3)">
    <!-- Large cloud -->
    <ellipse cx="120" cy="100" rx="40" ry="25"/>
    <ellipse cx="100" cy="90" rx="25" ry="18"/>
    <ellipse cx="140" cy="85" rx="30" ry="20"/>
    
    <!-- Medium cloud -->
    <ellipse cx="280" cy="80" rx="30" ry="18"/>
    <ellipse cx="265" cy="72" rx="18" ry="12"/>
    <ellipse cx="295" cy="70" rx="22" ry="14"/>
    
    <!-- Small cloud -->
    <ellipse cx="320" cy="160" rx="20" ry="12"/>
    <ellipse cx="310" cy="155" rx="12" ry="8"/>
    <ellipse cx="330" cy="153" rx="15" ry="9"/>
  </g>
  
  <!-- Data transfer arrows -->
  <g stroke="white" stroke-width="3" fill="none" opacity="0.8">
    <path d="M60 140 Q 100 120 140 140">
      <animate attributeName="stroke-dasharray" values="0,20;20,0;0,20" dur="2s" repeatCount="indefinite"/>
    </path>
    <path d="M180 120 Q 220 100 260 120">
      <animate attributeName="stroke-dasharray" values="0,20;20,0;0,20" dur="2.5s" repeatCount="indefinite"/>
    </path>
  </g>
  
  <!-- Server icons -->
  <g transform="translate(50, 180)">
    <rect x="0" y="0" width="60" height="40" rx="4" fill="rgba(255,255,255,0.2)"/>
    <rect x="8" y="8" width="44" height="8" rx="2" fill="rgba(255,255,255,0.4)"/>
    <rect x="8" y="20" width="44" height="8" rx="2" fill="rgba(255,255,255,0.4)"/>
    <circle cx="56" cy="12" r="2" fill="#10b981"/>
    <circle cx="56" cy="24" r="2" fill="#10b981"/>
  </g>
  
  <g transform="translate(290, 190)">
    <rect x="0" y="0" width="60" height="40" rx="4" fill="rgba(255,255,255,0.2)"/>
    <rect x="8" y="8" width="44" height="8" rx="2" fill="rgba(255,255,255,0.4)"/>
    <rect x="8" y="20" width="44" height="8" rx="2" fill="rgba(255,255,255,0.4)"/>
    <circle cx="56" cy="12" r="2" fill="#10b981"/>
    <circle cx="56" cy="24" r="2" fill="#10b981"/>
  </g>
  
  <!-- Text overlay -->
  <text x="200" y="280" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="24" font-weight="bold">Cloud Migration</text>
</svg>